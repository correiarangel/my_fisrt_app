{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "my_first_app",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "my_first_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "my_first_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "Development",
            "request": "launch",
            "type": "dart",
            "args": [
              "-t",
              "lib/main_dev.dart",
              "--flavor",
              "dev",
            ]
          },
          {
            "name": "Production",
            "request": "launch",
            "type": "dart",
            "args": [
              "-t",
              "lib/main_prod.dart",
              "--flavor",
              "prod",
            ]
          },
          {
            "name": "Homologation",
            "request": "launch",
            "type": "dart",
            "args": [
              "-t",
              "lib/main_hom.dart",
              "--flavor",
              "hom",
            ]
          },
          {
            "name": "Run all tests",
            "type": "dart",
            "request": "launch",
            "program": "test/",
          },
          {
            "name": "Run all tests with coverage",
            "type": "dart",
            "request": "launch",
            "program": "test/",
            "args": ["--coverage"],
          }
    ]
}